'use strict';

/**
 *  方法描述: 根据文件路径获取文件名
 *  作    者: 马军辉
 *  时    间: 2017-11-24
 *  参    数:
 */
export function getFileNameByUri(uri) {
//  file:///storage/emulated/0/Documents/IMG_20171124_140113.png
    if (!uri) return ""
    const arr = uri.split('/')
    return arr[arr.length - 1]
}